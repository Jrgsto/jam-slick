{
  "name": "template",
  "description": "template",
  "version": "0.0.1",
  "scripts": {
    "dev": "./node_modules/.bin/run-p watch:*",
    "watch:css": "./node_modules/.bin/postcss src/assets/global.css -o static/global.css -w",
    "watch:dev": "./node_modules/.bin/sapper dev",
    "build": "./node_modules/.bin/run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.css -o static/global.css",
    "build:sapper": "./node_modules/.bin/sapper build --legacy",
    "build:export": "./node_modules/.bin/sapper export --legacy",
    "export": "./node_modules/.bin/run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "../node_modules/.bin/run-p --race dev cy:run"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "axios": "^0.21.0",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "gray-matter": "^4.0.2",
    "marked": "^1.2.5",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-swipe": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "8.1.0",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2"
  }
}
